{
    "name": "dryck-syntax",
    "displayName": "Dryck Syntax",
    "description": "",
    "version": "0.5.0",
    "main": "./extension.js",
    "engines": {
        "vscode": "^1.68.0"
    },
    "activationEvents": [
        "onLanguage:dryck",
        "onLanguage:html-dryck"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/jleen/dryck-syntax"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dryck",
                "aliases": ["Dryck", "dryck"],
                "extensions": [".dryck"],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "html-dryck",
                "aliases": ["HTML (Dryck)", "html-dryck"],
                "extensions": [".html.dryck"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dryck",
                "scopeName": "source.dryck",
                "path": "./syntaxes/dryck.tmLanguage.json"
            },
            {
                "language": "html-dryck",
                "scopeName": "text.html.dryck",
                "path": "./syntaxes/html-dryck.tmLanguage.json"
            }
        ]
    }
}